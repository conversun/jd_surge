#!name=JD Cookie Sync - Config Panel
#!desc=Configuration panel for JD Cookie Sync (Optional)
#!system=ios

[Panel]
# 智能配置检查 - 根据配置状态自动显示配置向导或测试结果
jd_smart_check = script-name=config_helper_smart_check, title="JD Cookie Sync", content="点击检查配置", update-interval=-1

# 清除 Cookie 缓存
jd_clear_cache = script-name=config_helper_clear_cache, title="清除缓存", content="清除 Cookie 缓存"

[Script]
# 智能配置检查
config_helper_smart_check = type=generic, script-path=https://raw.githubusercontent.com/conversun/jd_surge/main/config_helper.js, argument=smart-check

# 清除 Cookie 缓存
config_helper_clear_cache = type=generic, script-path=https://raw.githubusercontent.com/conversun/jd_surge/main/config_helper.js, argument=clear-cache

